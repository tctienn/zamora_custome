extend type Query {
    getAllLabel: [Label]
}

extend type Mutation {
    labelSave(labelInput: LabelInput): Label
    removeLabelById(id: String): String
}

type Label {
    id: String,
    labelTitle: String,
    labelColor: String,
}

input LabelInput {
    id: String,
    labelTitle: String,
    labelColor: String,
}